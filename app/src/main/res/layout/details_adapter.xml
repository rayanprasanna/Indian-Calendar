<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_name"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="1dp"
            android:text="event on "
            android:textColor="#99696969"
            android:layout_marginTop="10dp"
            android:textSize="10sp"
            android:id="@+id/month"
            tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry,SmallSp,UnknownIdInLayout" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="10dp"
            android:text="event on "
            android:textColor="#000000"
            android:layout_marginTop="10dp"
            android:textSize="16sp"
            android:id="@+id/title"
            tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry,UnknownIdInLayout" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:background="#ffffff"
        android:layout_below="@+id/tv_name"
        android:id="@+id/visdf"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_height="1.5dp"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:id="@+id/img_save"
        android:layout_marginRight="20dp"
        android:layout_below="@+id/visdf"
        android:layout_marginLeft="20dp"
        android:visibility="gone"
        android:layout_marginTop="20dp"
        tools:ignore="ContentDescription" />


</RelativeLayout>